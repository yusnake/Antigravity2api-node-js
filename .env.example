# 服务配置
PORT=8045
HOST=0.0.0.0
# IMAGE_BASE_URL=http://your-domain.com  # 可选: 自定义图片访问基础 URL，默认使用本机或服务器 IP
MAX_IMAGES=10 # 本地最大保留的图片数量，超过会删除最早的

# API 配置
API_URL=https://daily-cloudcode-pa.sandbox.googleapis.com/v1internal:streamGenerateContent?alt=sse
API_MODELS_URL=https://daily-cloudcode-pa.sandbox.googleapis.com/v1internal:fetchAvailableModels
API_NO_STREAM_URL=https://daily-cloudcode-pa.sandbox.googleapis.com/v1internal:generateContent
API_HOST=daily-cloudcode-pa.sandbox.googleapis.com
API_USER_AGENT=antigravity/1.11.3 windows/amd64

# 默认参数
DEFAULT_TEMPERATURE=1
DEFAULT_TOP_P=0.85
DEFAULT_TOP_K=50
DEFAULT_MAX_TOKENS=8096

# 安全配置
MAX_REQUEST_SIZE=50mb
API_KEY=sk-text

# 调用日志设置
# REQUEST_LOG_LEVEL=all              # off=关闭, error=仅记录错误, all=记录全部请求
# REQUEST_LOG_MAX_ITEMS=200          # 最大保留调用日志条数（默认 200）
# REQUEST_LOG_RETENTION_DAYS=7       # 调用日志保留天数（默认 7）

# 其他网络配置
USE_NATIVE_AXIOS=false
TIMEOUT=180000
# PROXY=http://127.0.0.1:7897
CREDENTIAL_MAX_USAGE_PER_HOUR=20 # 单个凭证每小时调用上限，默认 20 次

# 系统提示词（留空将使用内置系统提示词）
SYSTEM_INSTRUCTION=

# 管理面板登录
PANEL_USER=admin
PANEL_PASSWORD=your-strong-password

# 自定义 Google OAuth 应用（可选）
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
